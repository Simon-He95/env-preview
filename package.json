{
  "publisher": "simonhe",
  "name": "env-preview",
  "displayName": "Env Preview",
  "version": "0.0.6",
  "packageManager": "pnpm@10.15.1",
  "description": "Instantly preview .env file variable values on hover in VS Code. Supports multiple .env files and provides quick edit/copy actions.",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/env-preview#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/env-preview"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/env-preview/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "keywords": [
    "env",
    ".env",
    "environment",
    "environment variables",
    "variables",
    "preview",
    "hover",
    "tooltip",
    "values",
    "inspector",
    "debug",
    "settings",
    "config",
    "configuration",
    "dotenv",
    "vite",
    "nextjs",
    "nodejs",
    "javascript",
    "typescript",
    "vscode",
    "vscode-extension",
    "developer-tools",
    "productivity"
  ],
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.75.1"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "dev": "nr build",
    "test": "vitest",
    "build": "tsdown",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "nr lint --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "nr build --minify && bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@types/node": "^18.19.123",
    "@types/vscode": "1.75.1",
    "@vscode-use/utils": "0.1.58",
    "@vscode/vsce": "^3.6.0",
    "bumpp": "^9.11.1",
    "dotenv": "^16.6.1",
    "eslint": "^9.34.0",
    "tinyglobby": "^0.2.14",
    "tsdown": "^0.9.9",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
